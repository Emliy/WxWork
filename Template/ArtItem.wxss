.news_content_onePic{margin: 0 auto;width:95%;height:200rpx;border-bottom:1px solid #f7f7f7;clear:both;}
.news_content_left{width:65%;float: left;position: relative;height: 180rpx;}
.news_content_left_top{height:auto;}
.news_content_left_but{font-size: 12px;position:absolute;bottom:0rpx;color:#ccc;}
.news_content_left_but .pic{width: 30rpx;height: 30rpx;border-radius: 15rpx;margin-right: 5px;margin-top:4rpx}
.news_content_left_top text.text_top{height: auto; padding: 34rpx 0 0 0;font-size:16px;line-height: 44rpx;color:#010101;display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;
    overflow: hidden;
    text-overflow:ellipsis;}
.news_content_right{height:180rpx;float: right;padding-right:3rpx;}
.news_content_right image{width:240rpx;height: 140rpx;margin: 40rpx 0 0 0rpx;}
.news_content-left{float: left;}
.news_content-right{float: right;}
/*无图*/
.news_content_noPic{margin: 0 auto;width:95%;height:188rpx;border-bottom:1px solid #f7f7f7;clear:both;}
.news_content_center{height:85px;padding-right:3rpx;overflow: hidden;}
.news_content_center image{width:220rpx;height: 140rpx;margin: 20rpx 25rpx 0 0rpx;}
.news_content_center image:last-child{width:220rpx;height: 140rpx;margin: 20rpx 0 0 0rpx;}
.news_content_bottom_but{font-size: 12px;color:#ccc;margin-top: 15rpx;}
.news_content_bottom_but .pic{width: 30rpx;height: 30rpx;border-radius: 15rpx;margin-right: 5px;margin-top:4rpx}
.news_content_noPic text.text_top{height: auto; padding: 20rpx 0 0 0;font-size:16px;line-height: 44rpx;color:#010101;display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;
    overflow: hidden;
    text-overflow:ellipsis;}
/*4图样式*/
.news_content_fourPic{margin: 0 auto;width:95%;height:auto;padding-bottom: 15rpx; border-bottom:1px solid #f7f7f7;clear:both;}
.news_content_center{height:85px;padding-right:3rpx;overflow: hidden;}
.news_content_center image{width:220rpx;height: 140rpx;margin: 20rpx 25rpx 0 0rpx;}
.news_content_center image:last-child{width:220rpx;height: 140rpx;margin: 20rpx 0 0 0rpx;}
.news_content_bottom_but{font-size: 12px;color:#ccc;}
.news_content_bottom_but .pic{width: 30rpx;height: 30rpx;border-radius: 15rpx;margin-right: 5px;margin-top:4rpx}
.news_content_fourPic text.text_top{height: auto; padding: 20rpx 0 0 0;font-size:16px;line-height: 44rpx;color:#010101;display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;
    overflow: hidden;
    text-overflow:ellipsis;}
/*视频文章样式*/
.news_content_video{margin: 0 auto;width:95%;height:100%;border-bottom:1px solid #f7f7f7;clear:both;}
.news_content_conVideo{height:400rpx;padding-right:3rpx;overflow: hidden;margin-top:15px;}
.news_content_conVideo video{width:99%;height:100%;}
.news_content_conVideo_bottom_but{font-size: 12px;color:#ccc;margin-top:10rpx;margin-bottom:10rpx;}
.news_content_conVideo_bottom_but .pic{width: 30rpx;height: 30rpx;border-radius: 15rpx;margin-right: 5px;margin-top:4rpx}
.news_content_fourPic text.text_top{height: auto; padding: 20rpx 0 0 0;font-size:16px;line-height: 44rpx;color:#010101;display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;
    overflow: hidden;
    text-overflow:ellipsis;}
/*一张大图文章样式*/
.news_content_video{margin: 0 auto;width:95%;height:100%;border-bottom:1px solid #f7f7f7;clear:both;}
.news_content_bigPic{height:400rpx;padding-right:3rpx;overflow: hidden;margin-top:15px;}
.news_content_bigPic image{width:100%;height:100%;}
.news_content_bigPic_bottom_but{font-size: 12px;color:#ccc;margin-top:10rpx;margin-bottom:10rpx;}
.news_content_bigPic_bottom_but .pic{width: 30rpx;height: 30rpx;border-radius: 15rpx;margin-right: 5px;margin-top:4rpx}
.news_content_fourPic text.text_top{height: auto; padding: 20rpx 0 0 0;font-size:16px;line-height: 44rpx;color:#010101;display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;
    overflow: hidden;
    text-overflow:ellipsis;}    
/*小视频封面图*/
.video-pic{position: relative;}
.video-pic .video-ico{position:absolute;bottom: 7rpx;right: 10rpx;background:rgba(0,0,0,0.4) ;border-radius: 15rpx;width: 100rpx;height: 40rpx;}
.video-pic .video-ico image.video-ico-sanjiao{width: 20rpx;height: 20rpx;margin: 10rpx 5rpx 0 8rpx;float: left;}
.video-pic .video-ico text.video-time{font-size: 10px;float: left;color: #fff;margin-top: 6rpx;}
.news_content_right image.video-bac-pic{width:240rpx;height: 140rpx;margin: 40rpx 0 0 0rpx;}

/*底部悬浮按钮*/
#comments{position: fixed;bottom: 0px;height:100rpx;width: 100%;background: #fff;border-top: 1px solid #eff2f7;}
#comments .pingLunInput{padding-top:5px;padding-left:10px;position: relative;float:left;}
#comments .pingLunInput .faBiao-ico{
  background:url(http://pic1.0086org.com/UF/Article/201801/yz.0086org.com/2456695/201801111108264747_44.png);
  background-position-x:30rpx;
  background-position-y:18rpx;
  background-size:45rpx;
  background-repeat:no-repeat;
  position:absolute;
  left:31rpx;
  top:7rpx;
  z-index:999999;
  width:80rpx;
  height:80rpx;

}
#comments .pingLunInput .FabiaoText{
  width:257rpx;
  height:76rpx;
  background:#fff;
  border-radius:22px;
  font-size:16px;
  text-indent:32rpx;
  float:left;
  line-height:76rpx;
  padding-left:100rpx;
  border:1px solid #eeeeee;
  color:#eee;
  }
#comments .pingLunLocation{position: relative;float:left;margin-top: 20rpx;margin-left: 28rpx;}
#comments .pingLunLocation .bubble{background: #e96067;color: #fff;text-align: center;font-size: 10px;position: absolute;top:-8rpx;
right:-12rpx;width:44rpx;border-radius:12rpx;}
#comments .pingLunLocation image{width: 55rpx;height: 55rpx;}
#comments .click{float: left;margin-left: 32rpx;margin-top: 20rpx;}
#comments .click image{width: 55rpx;height: 55rpx;}
#comments .Collection{float: left;margin-left: 28rpx;margin-top: 20rpx;}
#comments .Collection image{width: 55rpx;height: 55rpx;}
#comments button.share{float: left;margin-left: 28rpx;margin-top: 20rpx;background: url(http://pic1.0086org.com/UF/Article/201801/337/6666/2456695/pc/201801111448303691_255.png);background-size:55rpx;  width: 55rpx;height: 55rpx;border:0;}

/*评论区样式*/
.comments_list{padding:65rpx 30rpx 120rpx 30rpx;}
.comments_list .Hot-com{border-left: 2px solid #e96067;padding-left: 15rpx;font-size: 16px;margin-bottom:35rpx;}
.comments_list .comment-box{border-bottom: 1px solid #eff2f7;padding-top: 30rpx;padding-bottom:30rpx;}
.comments_list .comment-box .comment-head{height: 64rpx;}
.comments_list .comment-box .comment-head .com-pic{width: 60rpx; height: 60rpx;float: left;}
.comments_list .comment-box .comment-head .com-pic image{width: 60rpx; height: 60rpx;border-radius: 50%;display:block;}
.comments_list .comment-box .comment-head .com-name{float: left;color: #468cdd;font-size: 16px;margin-left: 15rpx;}
.comments_list .comment-box .comment-head .com-zan{float: right;}
.comments_list .comment-box .comment-head .com-zan image{width:55rpx;height:55rpx;float: left;}
.comments_list .comment-box .comment-head .com-zan text{font-size: 12px;color: #bfbfbf;padding-left: 10rpx;float: left;padding-top: 20rpx;}
.comments_list .comment-box .comment-cont{padding-left: 83rpx;}
.comments_list .comment-box .comment-cont text{font-size: 14px;line-height: 20px;}
.comments_list .comment-box .comment-footer{margin-top: 15rpx;}
.comments_list .comment-box .comment-footer .com-time{font-size: 12px;padding-left: 83rpx;}


